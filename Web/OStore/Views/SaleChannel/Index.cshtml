        <th></th>
    </tr>

    <div class="row">
        <div class="col-xs-12">
            <div class="panel panel-default">
                <div class="panel-body">
                    <ul class="nav nav-tabs">

                        <li>
                            <a href="#Manual" data-toggle="tab">Order manual</a>
                        </li>
                        <li>
